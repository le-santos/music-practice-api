<%= form_with model: [:web, @music] do |form| %>
  <%= form.label :title, t(:title) %>
  <%= form.text_field :title %>

  <%= form.label :composer, t(:composer) %>
  <%= form.text_field :composer %>

  <%= form.label :style, t(:style) %>
  <%= form.text_field :style %>

  <%= form.label :arranger, t(:arranger) %>
  <%= form.text_field :arranger %>

  <%= form.label :category, t(:category) %>
  <%= form.select :category, ['Solo', 'Duo'] %>

  <%= form.submit %>
<% end %>
